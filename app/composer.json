{
    "name": "wallbox/wallbox-php-backend-kata",
    "description": "PHP Backend Kata",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Kata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kata\\Tests\\": "tests/"
        }
    }
}
